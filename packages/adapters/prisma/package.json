{
    "name": "@mailien/prisma-adapter",
    "version": "0.0.1",
    "description": "Prisma database adapter for Mailien",
    "license": "MIT",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "prisma"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "lint": "tsc --noEmit",
        "clean": "rimraf dist"
    },
    "dependencies": {
        "@mailien/core": "workspace:*"
    },
    "devDependencies": {
        "@prisma/client": "^5.0.0",
        "prisma": "^5.0.0",
        "tsup": "^8.0.0",
        "rimraf": "^5.0.0",
        "typescript": "^5.5.0"
    },
    "peerDependencies": {
        "@prisma/client": ">=5.0.0"
    }
}